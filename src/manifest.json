{
	"name": "Bookmark Manager",
	"description": "Manages your bookmarks in a more user friendly way",
	"version": "0.1",
	"manifest_version": 2,
	"icons": {
		"16": "icons/bookmark-manager-chrome-16.png",
		"128": "icons/bookmark-manager-chrome-128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "icons/bookmark-manager-chrome-16.png"
		},
		"default_title": "Bookmark manager",
		"default_popup": "index.html"
	},
	"permissions": [
		"tabs",
		"storage",
		"bookmarks"
	]
}
